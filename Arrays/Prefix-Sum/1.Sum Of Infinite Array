def sumInRanges(arr, n, queries, q): #todo bruteforce better and simple
    res = []
    for query in queries:
        l = query[0]-1
        r = query[1]
        s = 0
        for i in range(l,r):
            s += arr[i % n]
        res.append(s)
    return res
